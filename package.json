{
  "name": "@codeiqlabs/aws-cdk",
  "version": "1.0.0",
  "description": "CodeIQLabs AWS CDK wrapper utilities for Level 1 and Level 2 abstractions",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./l1": {
      "types": "./dist/l1/index.d.ts",
      "import": "./dist/esm/l1/index.js",
      "require": "./dist/cjs/l1/index.js"
    },
    "./l2": {
      "types": "./dist/l2/index.d.ts",
      "import": "./dist/esm/l2/index.js",
      "require": "./dist/cjs/l2/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "husky",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:status": "changeset status",
    "changeset:check": "find .changeset -name '*.md' ! -name 'README.md' ! -name 'RELEASE_TEMPLATE.md' | wc -l",
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs:pkg": "node -e \"require('fs').mkdirSync('dist/cjs',{recursive:true});require('fs').writeFileSync('dist/cjs/package.json','{\\\"type\\\":\\\"commonjs\\\"}\\n')\"",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:cjs:pkg && npm run lint",
    "build:cjs:show": "tsc -p tsconfig.cjs.json --showConfig",
    "test:load": "node tests/load-config.test.cjs",
    "test:esm": "node tests/esm-import-test.mjs",
    "test:all": "npm run test:load && npm run test:esm",
    "prepublishOnly": "npm run build && npm run test:all",
    "release": "changeset publish"
  },
  "keywords": [
    "aws",
    "cdk",
    "codeiqlabs",
    "infrastructure",
    "typescript",
    "wrappers"
  ],
  "author": "CodeIQLabs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeIQLabs/codeiqlabs-aws-cdk"
  },
  "dependencies": {
    "@codeiqlabs/aws-utils": "file:../codeiqlabs-aws-utils"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@codeiqlabs/eslint-prettier-config": "^1.4.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "aws-cdk": "2.123.0",
    "aws-cdk-lib": "2.208.0",
    "constructs": "^10.0.0",
    "eslint": "^9.33.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.208.0",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
